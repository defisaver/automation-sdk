{
  "name": "@defisaver/automation-sdk",
  "version": "3.1.4-dev-5",
  "description": "",
  "main": "./cjs/index.js",
  "module": "./esm/index.js",
  "types": "./esm/index.d.ts",
  "scripts": {
    "version-bump": "git commit -am \"Version bump to $(npm version patch | cut -c 2-)\"",
    "build:esm": "rm -rf esm && tsc -p tsconfig.esm.json",
    "build:cjs": "rm -rf cjs && tsc -b",
    "build": "npm run lint && npm run build:cjs && npm run build:esm",
    "documentMD": "npx typedoc --plugin typedoc-plugin-markdown --out docs/md",
    "document": "npx typedoc --out docs/default",
    "lint": "eslint ./src --fix",
    "lint-check": "eslint src/",
    "generate-contracts": "node scripts/generateContractTypes.js",
    "prepare": "husky install && npm run generate-contracts",
    "test": "mocha ./src/**/*.test.ts",
    "test-single": "mocha ./src/**/$npm_config_name.test.ts"
  },
  "author": "DeFi Saver",
  "repository": "https://github.com/defisaver/automation-sdk",
  "license": "ISC",
  "dependencies": {
    "@defisaver/eslint-config": "^1.0.1",
    "@defisaver/sdk": "^1.2.16-dev-liquity-v2-6",
    "@defisaver/tokens": "^1.5.52-dev-5",
    "@ethersproject/address": "^5.0.10",
    "@ethersproject/solidity": "^5.0.9",
    "decimal.js": "^10.4.3",
    "lodash": "^4.17.21",
    "web3-eth-abi": "^1.10.2",
    "web3-utils": "^1.10.2"
  },
  "devDependencies": {
    "@defisaver/eslint-config": "^1.0.1",
    "@types/chai": "^4.3.6",
    "@types/lodash": "^4.14.191",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.11.15",
    "@typescript-eslint/parser": "^5.42.0",
    "chai": "^4.3.8",
    "dotenv": "^16.3.1",
    "eslint": "^8.49.0",
    "husky": "^8.0.0",
    "jsdoc-to-markdown": "^6.0.1",
    "mocha": "^10.2.0",
    "react": "^18.2.0",
    "ts-node": "^10.9.1",
    "typechain": "^8.1.0",
    "typechain-target-web3-v1-3mihai3": "^6.0.1",
    "typedoc": "^0.23.20",
    "typedoc-plugin-markdown": "^3.13.6",
    "typescript": "^4.8.4"
  },
  "peerDependencies": {
    "web3": "^1.10.2",
    "web3-eth-contract": "^1.8.1"
  }
}
